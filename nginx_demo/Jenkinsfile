pipeline {
    agent any

    stages {
        stage('scm checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/Alok1408/New-repo_jk.git'
            }
        }
        stage('docker image build') {
            steps {
              sh'docker build -f Dockerfile.base -t alok1408/nginx .'
            }
        }
    }
}

